{
  "name": "esri-map",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "docs": "ng build --prod --output-path docs --base-href esri-map && cp docs/index.html docs/404.html",
    "build-lib": "ng build ng-esri-map",
    "npm-pack": "cd dist/ng-esri-map && npm pack",
    "package": "npm run build-lib && npm run npm-pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@types/arcgis-js-api": "^4.9.0",
    "core-js": "^2.5.4",
    "esri-loader": "^2.5.0",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.8",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "ng-packagr": "^4.1.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
