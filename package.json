{
  "name": "esri-map",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "docs": "ng build --prod --output-path docs --base-href esri-map && cp docs/index.html docs/404.html",
    "build-lib": "ng build ng-esri-map",
    "npm-pack": "cd dist/ng-esri-map && npm pack",
    "package": "npm run build-lib && npm run npm-pack"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@types/arcgis-js-api": "4.10.0",
    "core-js": "2.6.2",
    "esri-loader": "2.6.0",
    "rxjs": "6.3.3",
    "tslib": "1.9.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.1",
    "@angular-devkit/build-ng-packagr": "0.12.1",
    "@angular/cli": "7.2.1",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@types/node": "10.12.18",
    "codelyzer": "4.5.0",
    "ng-packagr": "4.4.5",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  }
}
